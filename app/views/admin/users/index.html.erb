<% provide :title, t("allusers") %>
<h1><%= t :allusers %></h1>
<div class="row">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      <%= link_to t(:delete), [:admin, user], method: :delete, data: {confirm: t(:areyousure)} %>
      <%= link_to t(:edituser), edit_admin_user_path(user) %>
    </li>
  <% end %>
  <%= will_paginate %>
</div>
