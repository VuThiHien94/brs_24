<% provide :title, t("allusers") %>
<h1><%= t :allusers %></h1>
<div class="row">
  <% @users.each do |user| %>
    <div class="user">
      <%= link_to user.name, user %>
      <%= link_to t(:delete), [:admin, user], method: :delete,
        data: {confirm: t(:areyousure)}, class: "btn btn-danger float-right" %>
      <%= link_to t(:edituser), edit_admin_user_path(user),
        class: "btn btn-primary" %>
    </div>
  <% end %>
  <%= will_paginate %>
</div>
